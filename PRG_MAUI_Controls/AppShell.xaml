<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PRG_MAUI_Controls.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PRG_MAUI_Controls"
    Shell.FlyoutBehavior="Disabled"
    Title="PRG_MAUI_Controls">

    <TabBar>
        <ShellContent Title="Start" Route="Main" ContentTemplate="{DataTemplate local:Main}" />
        <ShellContent Title="In &amp; output" Route="Inoutput" ContentTemplate="{DataTemplate local:Inoutput}" />
        <ShellContent Title="Text" Route="Text" ContentTemplate="{DataTemplate local:Text}" />
        <ShellContent Title="Bilder" Route="Images" ContentTemplate="{DataTemplate local:Images}" />
    </TabBar>

</Shell>